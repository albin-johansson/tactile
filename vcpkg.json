{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "tactile",
  "license": "GPL-3.0",
  "version-semver": "0.5.0",
  "maintainers": [
    "albin-johansson"
  ],
  "builtin-baseline": "3f806e04fdcf30df33c7b445590c9ac13afef946",
  "dependencies": [
    "argparse",
    "boost-stacktrace",
    "boost-uuid",
    "entt",
    "cppcodec",
    "fast-float",
    "fmt",
    {
      "name": "glad",
      "features": [
        "extensions"
      ]
    },
    "gtest",
    {
      "name": "imgui",
      "features": [
        "docking-experimental",
        "sdl2-binding",
        "opengl3-binding"
      ]
    },
    "magic-enum",
    "nlohmann-json",
    "protobuf",
    "pugixml",
    {
      "name": "sdl2",
      "features": [
        "vulkan"
      ]
    },
    "sdl2-image",
    "stb",
    "tinyfiledialogs",
    "tl-expected",
    "yaml-cpp",
    "zlib",
    "zstd"
  ]
}
